<manifest android:versionName="0.1" package="com.entscheidungsbaum.nativesound" android:versionCode="0" xmlns:android="http://schemas.android.com/apk/res/android">

    <application android:debuggable="true" android:label="@string/app_name" android:icon="@drawable/android:star_big_on">

        
        <uses-permission android:name="android.permission.INTERNET"></uses-permission>
        <uses-permission android:name="android.permission.RECORD_AUDIO"></uses-permission>
        <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
        <uses-permission android:name="android.permission.VIBRATE"></uses-permission>
        <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

        <activity android:name=".SoundMainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:configChanges="orientation|keyboardHidden" android:name=".RecordActivity"></activity>
    </application>

    <uses-sdk android:minSdkVersion="10"></uses-sdk>
</manifest>